{
  "name": "html-reactifier",
  "version": "1.2.0-alpha",
  "description": "Given a valid HTML snippet, this library will return a render function to render the snippet using react.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "make test",
    "start": "make check",
    "prepublish": "make"
  },
  "repository": {
    "type": "git",
    "url": "jsg2021/html-reactifier"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "html",
    "react",
    "template",
    "widget"
  ],
  "author": "Jonathan Grimes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsg2021/html-reactifier/issues"
  },
  "homepage": "https://github.com/jsg2021/html-reactifier#readme",
  "dependencies": {
    "elementtree": "^0.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.1.0",
    "eslint": "^4.12.1",
    "jest": "^21.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testMatch": [
      "**/__test__/**/*.(spec).js?(x)"
    ]
  }
}
