{
  "name": "html-reactifier",
  "version": "2.0.2",
  "description": "Given a valid HTML snippet, this library will return a render function to render the snippet using react.",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "make test",
    "start": "make check",
    "prepack": "make"
  },
  "repository": {
    "type": "git",
    "url": "jsg2021/html-reactifier"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "html",
    "react",
    "template",
    "widget"
  ],
  "author": "Jonathan Grimes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsg2021/html-reactifier/issues"
  },
  "homepage": "https://github.com/jsg2021/html-reactifier#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.1",
    "eslint": "^8.0.0",
    "jest": "^28.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testMatch": [
      "**/__test__/**/*.(spec).js?(x)"
    ]
  }
}
