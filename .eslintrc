{
	"extends": "eslint:recommended",

	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"impliedStrict": true,
			"globalReturn": false
		}
	},

	"env": {
		"node": true,
		"browser": true,
		"jasmine": true,
		"es6": true
	}
}
